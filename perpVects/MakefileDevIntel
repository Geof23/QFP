CC := icpc

CC_OPTS ?= -Wuninitialized

CC_REQUIRED := -lm --std=c++11 -lstdc++ -Wno-shift-count-overflow -gxx-name=g++-5.1 -frounding-math

INCFILES := ../gdb/inst

INC := -I $(INCFILES)



.phony : all

all : intel #intel_g

intel : test.cpp $(INCFILES)
	$(CC) $(CC_OPTS) $(CC_REQUIRED) $(INC) $< -o $@

# intel_g : test.cpp $(INCFILES)
# 	$(CC) $(CC_OPTS) -g $(CC_REQUIRED) $(INC) $< -o $@

.phony : clean

clean :
	-rm $(CC)*
